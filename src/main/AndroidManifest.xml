<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartcoffee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DesalinationFinished"></activity>
        <activity android:name=".Desalination" />
        <activity android:name=".WaterNotification" />
        <activity android:name=".StepThree" />
        <activity android:name=".StepTwo" />
        <activity android:name=".StepOne" />
        <activity
            android:name=".CoffeeType"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".CustomCoffee" />
        <activity
            android:name=".WaterSettings"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".CoffeeChoice" />
        <activity android:name=".Favorite" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>